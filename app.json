      "supportsTablet": true,
      "bundleIdentifier": "com.joaquinsaavedra.finalProjectSaavedra",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "##bb756e"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.joaquinsaavedra.finalProjectSaavedra"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "6e15558a-3c59-4256-ba86-f42ba6bcb3aa"
      }
    },
    "owner": "joaquinsaavedrae"
  }
